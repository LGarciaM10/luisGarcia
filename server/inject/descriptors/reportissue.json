{
  "type": "object",
  "comment": "reportissue",
  "properties": {
    "category": {
      "type": "select",
      "update": {
        "type": "none"
      },
      "selector": "[name=\"category_id\"]",
      "control": "select"
    },
    "priority": {
      "type": "select",
      "update": {
        "type": "none"
      },
      "selector": "[name=\"priority\"]",
      "control": "select"
    },
    "summary": {
      "type": "string",
      "update": {
        "type": "none"
      },
      "selector": "[name=\"summary\"]",
      "control": "text"
    },
    "description": {
      "type": "string",
      "update": {
        "type": "none"
      },
      "selector": "[name=\"description\"]",
      "control": "text"
    },
    "isReportIssue": {
      "type": "identifier",
      "control": "exists",
      "selector": ".form-title",
      "selectContains": "Enter Report Details"
    },
    "submit": {
      "type": "action",
      "selector": "input.button[value=\"Submit Report\"]",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isReportIssue"
      ],
      "screens": [
        "reportissue"
      ]
    }
  ]
}
